<xml>
    <object name="AeroSettings" singleinstance="true" settings="true" category="Sensors">
        <description>Settings for the @ref Aero module</description>
        <field name="Mass" units="g" type="uint16" elements="1" defaultvalue="1500" description="Model weight (g)"/>
        <field name="WingSurface" units="dm^2" type="uint16" elements="1" defaultvalue="35" description="Wing area (dm2)"/>
        <field name="Rho" units="Kg/m^3" type="float" elements="1" defaultvalue="1.2" description="Air density Kg/m3" />
        <field name="MeasurementPeriod" units="ms" type="uint8" elements="1" defaultvalue="100"/>
        <field name="StartCalcSource" units="" type="enum" elements="1" options="Disabled,Accessory0,Accessory1,Accessory2,Accessory3," defaultvalue="Disabled" description="Choose Accessory to start measurement, Disabled do calc all times"/>
        <field name="NzLowPassAlpha" units="" type="float" elements="1" defaultvalue="0.7" description="Nz (normal acceleration) filtering. 0 to disable" />
        <field name="AirSpeedLowPassAlpha" units="" type="float" elements="1" defaultvalue="0.7" description="Airspeed filtering. 0 to disable" />
        <field name="ClLimits" units="coeff" type="float" elementnames="Min,Max,Resolution" defaultvalue="-1.2,1.2,0.05" description="Lift coefficient limits" />
        <field name="NzLimits" units="G" type="float" elementnames="Min,Max" defaultvalue="-10,10" description="Nz limits (G force)" />
        <access gcs="readwrite" flight="readwrite"/>
        <telemetrygcs acked="true" updatemode="onchange" period="0"/>
        <telemetryflight acked="true" updatemode="onchange" period="0"/>
        <logging updatemode="manual" period="0"/>
	</object>
</xml>
